<% Array(@photos).each do |photo| %>     
<div class="image-item span4" data-photo-url="<%= photo.photo.url %>"> 
	<div class="slide" id="slider">
		<ul class="unstyled">
			<li class="image" style="display:block;position:relative">
				<div class="hidden-phone hidden-tablet photo-mask">                           
					<div class="share-buttons">
						<a href="#" class="btn share-on-fb btn-success">Share on Facebook</a>
					 	<a href="#" class="btn share-on-tw btn-success">Share on Twitter</a>
						<a href="#" class="btn share-on-g btn-success">Share on G+</a>
					</div>
				</div>
				<img src="<%= photo.photo.url %>" alt="Funny picture" style="margin:0px auto;"/>							
				<div class="hidden-desktop" style="position:absolute;color:white;font-size:48px;font-weight:bold;right:20px;top:50%;text-shadow:1px 1px 2px rgba(0,0,0,0.8);letter-spacing:-4px">&laquo;</div>
			</li>                                                      
			<li class="services" style="display:none">
				<div style="padding:10px">
					<div class="share-buttons-phone">
						<a href="#" class="btn share-on-fb btn-success btn-large">Share on Facebook</a><br /><br />
					 	<a href="#" class="btn share-on-tw btn-success btn-large">Share on Twitter</a><br /><br />
						<a href="#" class="btn share-on-g btn-success btn-large">Share on G+</a>
					</div>
				</div>
			</li>
		</ul>              
	</div>
</div>		
<% end %>